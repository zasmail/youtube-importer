{{#paper-menu as |menu|}}
  {{#menu.trigger}}
    {{#paper-button iconButton=true}}
      {{paper-icon "list"}}
    {{/paper-button}}
  {{/menu.trigger}}
  {{#menu.content width=4 as |content|}}
    {{#each items as |item|}}
      {{#content.menu-item onClick=item.route}}
        {{paper-icon item.icon}}
        <span>{{item.title}}</span>
      {{/content.menu-item}}
    {{/each}}
  {{/menu.content}}
{{/paper-menu}}
